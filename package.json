{
  "name": "",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "https://pearson-higher-ed.github.io/component-archetype/",
  "main": "./build/dist.component-name.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "NODE_ENV=development webpack-dev-server --hot",
    "gh-pages": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/gh-pages.js",
    "test": "jest --coverage",
    "build": "NODE_ENV=production webpack -p",
    "gen-changelog": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/gen-changelog.js",
    "version": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/version.js",
    "ci": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/ci.js",
    "release": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/release.js",
    "verify": "node ./node_modules/@pearson-components/npm-scripts/npm_scripts/verify.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pearson-Higher-Ed/component-archetype/"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg|png)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "collectCoverageFrom": [
      "**/src/js/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "testPathIgnorePatterns": [
      "<rootDir>/(build|node_modules|demo|coverage)/"
    ],
    "verbose": true
  },
  "devDependencies": {
    "@pearson-components/npm-scripts": "^0.5.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.5.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "~3.16.1",
    "file-loader": "^0.10.0",
    "fs-extra": "^1.0.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "16.0.2",
    "node-sass": "^4.5.0",
    "pearson-elements": "^1.11.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-intl": "^2.0.1",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "uuid": "2.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "keywords": []
}
